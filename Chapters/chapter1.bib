keywords = {{\#}ElGordo{\_}paper1,{\#}thesis{\_}ch1,markov,metropolis algorithm,optimal scaling,weak convergence},
mendeley-tags = {{\#}ElGordo{\_}paper1,{\#}thesis{\_}ch1},
